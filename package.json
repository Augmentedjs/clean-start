{
  "name": "clean-start",
  "version": "1.2.0",
  "description": "A simple clean webpack 5, babel 8, sass, css, and externalized stylesheet project setup for use",
  "main": "src/index.js",
  "scripts": {
    "dev": "clear && webpack --mode development",
    "build": "clear && webpack --mode production",
    "clean": "rm -rf node_modules",
    "test": "mocha --require @babel/register --require test/helper.js -c test/*Spec.js",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/clean-start.git"
  },
  "keywords": [
    "setup",
    "webpack",
    "babel",
    "mocha",
    "chai",
    "sass",
    "css"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Augmentedjs/clean-start/issues"
  },
  "bin": {
    "clean-start": "./bin/cs.js"
  },
  "preferGlobal": true,
  "homepage": "https://github.com/Augmentedjs/clean-start#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "^5.3.0",
    "jsdom": "^16.5.0",
    "mini-css-extract-plugin": "^1.3.9",
    "mocha": "^8.3.1",
    "node-sass": "^5.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  }
}
